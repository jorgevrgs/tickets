{
    "name": "jorgevrgs/tickets",
    "description": "Tickets in front office for customers in PrestaShop",
    "type": "prestashop-module",
    "license": "MIT",
    "authors": [
        {
            "name": "Jorge Vargas",
            "email": "jorgevargasla@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "config": {
      "preferred-install": "dist",
      "prepend-autoloader": false
    },
    "require": {},
    "require-dev": {
        "phpunit/phpunit": "^8.3"
    },
    "autoload": {
        "psr-4": {
          "App\\Tickets\\": "src/"
        },
        "classmap": [
          "classes/",
          "models/"
        ],
        "exclude-from-classmap": []
      },
      "autoload-dev": {
        "psr-4": {
          "App\\Tickets\\Tests\\": "tests/"
        }
      }
}
